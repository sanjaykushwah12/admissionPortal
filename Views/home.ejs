<%- include('header.ejs') %>
<div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
  <div class="carousel-inner ">
    <div class="carousel-item active">
      <img src="image/library.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="image/clg.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="image/ground.jpg" class="d-block w-100" alt="...">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>



<div class="container">

<h2 class="text-center py-3">Hello <%= n %></h2>
 <h2 class="text-center">REGISTER YOUR COURSE</h2>
 <p class="text-center mb-4">Course Registration means the official recording of a student's name to study a course or courses</p>
 


 <div class="row">

  <div class="col-md-4 mb-5">
      <div class="card card1">
          <img class="card-img-top" height="200px" width="300px" src="image/btech2.jpg" alt="Card image cap">
          <div class="card-body">
              <h5 class="card-title text-center ">B.Tech</h5>
              <p class="card-text text-center">B.Tech is a professional undergraduate engineering degree</p>
              <% if(bt){ %>  
                      <a href="course/btech/display" class="btn btn-primary card123">Display</a>
              <% } else{ %> 
                <a href="course/btech" class="btn btn-primary card123">Enroll Now</a>
             <%  } %>        
          </div>
      </div>
  </div>

 
  <div class="col-md-4 mb-4">
      <div class="card card3">
          <img class="card-img-top " height="200px" width="300px" src="image/mca.jpg" alt="Card image cap">
          <div class="card-body">
              <h5 class="card-title text-center ">MCA</h5>
              <p class="card-text text-center">MCA is a professional post-graduation degree in computer science</p>
              
              <% if(mca1){ %>  
                <a href="course/mca/display" class="btn btn-primary card123">Display</a>
              <% } else{ %> 
                <a href="course/mca" class="btn btn-primary card123">Enroll Now</a>
              <%  } %>   
              
              

                      
          </div>
      </div>
  </div>

  <div class="col-md-4 mb-4">
    <div class="card card2">
        <img class="card-img-top " height="200px!important" width="300px!important" src="image/mba.jpg" alt="Card image cap">
        <div class="card-body">
            <h5 class="card-title text-center">MBA</h5>
            <p class="card-text text-center">MBA program gives you fundamental management knowledge </p>
            <% if(mba1){ %>  
              <a href="course/mba/display" class="btn btn-primary card123">Display</a>
            <% } else{ %> 
                  <a href ="course/mba" class="btn btn-primary card123">Enroll Now</a>
               <%  } %> 
                   
                    
        </div>
    </div>
</div>


</div>
 </div>
































